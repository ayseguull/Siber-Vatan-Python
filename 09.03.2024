def fibonacci(n):
    if n <= 0:
        print("pozitif tam sayı olması gerekk")
    liste = []
    a, b = 0, 1
    for _ in range(n):
        liste.append(a)
        a, b = b, a + b
    return liste

n = 10
print(fibonacci(n))
print()
