def palindrom_mu(metin):
    temiz_metin = metin.upper()
    return temiz_metin == temiz_metin [::-1]

metin = input("metni girin: ")
if palindrom_mu(metin):
    print("bu palidnrom bi kelimedir")
else:
    print("değilfir")

