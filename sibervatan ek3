liste1=[1,2,3,4,5]
liste2=[3,4,5,6,7]

ortak_elemanlar = []

for eleman in liste1:
    if eleman in liste2:
        ortak_elemanlar.append(eleman)

print("ortak elemanlar şulardır:", ortak_elemanlar)
#yol iki

""
def fibonacci(n):
    if n <= 0:
        print("pozitif tam sayı olması gerekk")
    liste = []
    a, b = 0, 1
    for _ in range(n):
        liste.append(a)
        a, b = b, a + b
    return liste

n = 10
print(fibonacci(n))
print(n)
#benim yaptıgım
""
def fibonacci(n):
    fibo= [0,1]

    for i in range(2,n):
        next= fibo[-1] + fibo[-2]
        fibo.append(next)
    return fibo
n = 100
fibo = fibonacci(n)
print(f"{fibo}")
#hocanın yaptığı
